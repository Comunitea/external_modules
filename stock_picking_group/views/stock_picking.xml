<?xml version="1.0" encoding="utf-8"?>
<odoo>

     <record id="view_picking_type_form" model="ir.ui.view">
        <field name="name">stock.picking.type.form.grouo</field>
        <field name="model">stock.picking.type</field>
        <field name="inherit_id" ref="stock.view_picking_type_form"/>
        <field name="arch" type="xml">
            <field name="show_reserved" position="after">
                <field name="grouped_field_ids" widget="many2many_tags" groups="stock.group_stock_multi_locations"/>
                <field name="grouped_batch_field_ids" widget="many2many_tags" groups="stock.group_stock_multi_locations"/>
                <field name="after_assign" groups="stock.group_stock_multi_locations"/>
                <!--field name="not_grouped_field_ids" widget="many2many_tags" groups="stock.group_stock_multi_locations"/-->
            </field>
            <field name="sequence_id" position="after">
                <field name="batch_picking_sequence_id"/>
                <field name="parent_id"/>
            </field>
        </field>
    </record>
</odoo>
