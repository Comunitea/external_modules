<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

    <t t-name="Menu" t-inherit="web.Menu" t-inherit-mode="extension" owl="1">
        <xpath expr="//nav[hasclass('o_main_navbar')]" position="before">
            <div id="backend_company_logo_row" class="container-fluid d-none d-md-block mt-1 mb-1">
                <a href="#">
                    <img class="img img-fluid" id="backend_company_logo" style="max-width: 40px;" t-attf-src="/web/binary/company_logo" alt="Logo"/>
                </a>
            </div>
        </xpath>
    </t>

</templates>
