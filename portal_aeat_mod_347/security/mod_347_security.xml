<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <record id="l10n_es_aeat_mod347_partner_record_portal" model="ir.rule">
            <field name="name">AEAT Mod 347 partner record portal access</field>
            <field ref="l10n_es_aeat_mod347.model_l10n_es_aeat_mod347_partner_record" name="model_id"/>
            <field name="groups" eval="[(4, ref('base.group_portal'))]"/>
            <field name="perm_unlink" eval="False"/>
            <field name="perm_write" eval="False"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_create" eval="False"/>
            <field name="domain_force">[('partner_id','child_of',[user.commercial_partner_id.id])]</field>
        </record>

        <record id="l10n_es_aeat_mod347_move_record_portal" model="ir.rule">
            <field name="name">AEAT Mod 347 move record portal access</field>
            <field ref="l10n_es_aeat_mod347.model_l10n_es_aeat_mod347_move_record" name="model_id"/>
            <field name="groups" eval="[(4, ref('base.group_portal'))]"/>
            <field name="perm_unlink" eval="False"/>
            <field name="perm_write" eval="False"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_create" eval="False"/>
            <field name="domain_force">[('partner_record_id','child_of',[user.commercial_partner_id.id])]</field>
        </record>

    </data>
</odoo>
